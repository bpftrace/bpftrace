NAME kretprobe:_raw_spin_lock is banned
RUN bpftrace -e 'kretprobe:_raw_spin_lock { exit(); }'
EXPECT error: kretprobe:_raw_spin_lock can't be used as it might lock up your system.
TIMEOUT 1

