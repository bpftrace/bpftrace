NAME assert_macro
PROG BEGIN { assert(false, "My error message"); }
EXPECT stdlib/base.bt:5:5-37: ERROR: assert failed: My error message
WILL_FAIL

NAME ppid_macro
PROG BEGIN { printf("SUCCESS %d\n", ppid(curtask)); exit(); }
EXPECT_REGEX SUCCESS [0-9][0-9]*

NAME strcontains_types
PROG BEGIN { strcontains("foo", 1) }
EXPECT_REGEX .* ERROR:.*
WILL_FAIL

NAME strcontains_not_present
PROG BEGIN { $a = "bar"; print(strcontains("foo", $a)) }
EXPECT false

NAME strcontains_present
PROG BEGIN { $a = "bar"; print(strcontains("foobar", $a)) }
EXPECT true
