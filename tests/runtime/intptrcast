AFTER ./testprogs/intptrcast

NAME Casting ints
RUN bpftrace -v -e 'uprobe:./testprogs/intptrcast:fn { $a=*(uint16*)(reg("sp")+8); printf("%d\n", $a); exit();}'
EXPECT 3258
TIMEOUT 5
ARCH x86_64
AFTER ./testprogs/intptrcast

