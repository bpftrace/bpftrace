NAME Sum casted value
RUN bpftrace -v -e 'uprobe:./testprogs/intptrcast:fn { @=sum(*(uint16*)(reg("sp")+8)); exit();}'
EXPECT ^@: 3258
TIMEOUT 5
ARCH x86_64
AFTER ./testprogs/intptrcast

NAME Sum casted value
RUN bpftrace -v -e 'uprobe:./testprogs/intptrcast:fn { @=sum(*(uint16*)(reg("sp")+0)); exit();}'
EXPECT ^@: 3258
TIMEOUT 5
ARCH aarch64
AFTER ./testprogs/intptrcast

