ARCH x86_64|aarch64|ppc64le
TIMEOUT 5

NAME ntop static ip
PROG i:ms:100 { printf("IP: %s, %s, %s, %s\n", ntop(0x01000000), ntop(0x7f000001), ntop(0x0000ffff), ntop(0xffff0000)); exit() }
