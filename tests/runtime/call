EXPECT 3412
TIMEOUT 1

NAME bswap_int8
RUN {{BPFTRACE}} -e 'BEGIN { printf("%x", bswap((int8)0x12)); exit(); }'
EXPECT 12
TIMEOUT 1

NAME bswap_int64_op
RUN {{BPFTRACE}} -e 'BEGIN { printf("%lx", bswap(0x12340000 + 0x5678)); exit(); }'
EXPECT 7856341200000000
TIMEOUT 1

NAME bswap_twice
RUN {{BPFTRACE}} -e 'BEGIN { printf("%x", bswap(bswap(0x1234))); exit(); }'
EXPECT 1234
TIMEOUT 1