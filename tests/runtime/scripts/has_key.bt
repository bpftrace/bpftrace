begin {
    $count = 0;
    // Test basic functionality
    @a[1] = 0;
    if (has_key(@a, 1)) {
        ++$count;
    }

    if (!has_key(@a, 2)) {
        ++$count;
    }

    $b = 1;
    if (has_key(@a, $b)) {
        ++$count;
    }

    @b["longerstr"] = 0;
    if (!has_key(@b, "byeasdfasdfasdfasdfasdfasdf")) {
        ++$count;
    }

    @b["longerstr"] = 0;
    if (has_key(@b, "longerstr")) {
        ++$count;
    }

    // Test a more complex key
    @c[1, ("hello", (int8)5)] = 0;
    if (has_key(@c, (1, ("hello", 5)))) {
        ++$count;
    }

    if (!has_key(@c, (1, ("bye", (int8)5)))) {
        ++$count;
    }

    print($count)
}
