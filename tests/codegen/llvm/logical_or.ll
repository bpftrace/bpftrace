  %2 = zext i32 %pid to i64
  %3 = icmp eq i64 %2, 1234
  %lhs_true_cond = icmp ne i1 %3, false
  br i1 %lhs_true_cond, label %"||_true", label %"||_lhs_false"

"||_lhs_false":                                   ; preds = %entry
