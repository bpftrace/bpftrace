test:basic_tuple
{
  $v = 99;
  $t = (0,1,"str",(5,6),$v);
  if ($t.0 != 0 ||
      $t.1 != 1 || $t.2 != "str" || $t.3.0 != 5 || $t.3.1 != 6 || $t.4 != 99) {
    return 1;
  }
}

test:basic_tuple_map
{
  $v = 99;
  @t = (0,1,"str");
  if (@t.0 != 0 || @t.1 != 1 || @t.2 != "str") {
    return 1;
  }
}

test:tuple_map_key_same_as_assoc_array
{
  @[1,"hello"] = 1;
  @[2,"hello"] = 2;
  delete(@, (1,"hello"));
  if (@[2,"hello"] != 2) {
    return 1;
  }
}

test:tuple_map_key_variable
{
  $a = (1,"hello");
  @[1,"hello"] = 1;
  @[$a] = 2;
  if (@[$a] != 2) {
    return 1;
  }
}

test:tuple_map_key_compatible_int_sizes
{
  $a = (1,(123,((uint64)1234)));
  $b = (4,(1234,((uint8)123)));
  @a[$a] = 1;
  @a[$b] = 2;
  if (@a[$a] != 1 || @a[$b] != 2) {
    return 1;
  }
}

