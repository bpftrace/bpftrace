test:complex_tuple_4
{
  $a = (((int8)(-100)),((int8)100),
        "abcdef",3,((int32)1),((int64)(-10)),((int8)10),((int16)(-555)),"abc");
  if (sizeof($a) != 32) {
    return 1;
  }
}

test:array_style_tuple_access
{
  $a = (1,2,3);
  if ($a[(1 + 1)] != 3) {
    return 1;
  }
}

test:tuple_binop_equals
{
  $x = ("hello",(-6));
  $y = ("hello",(-6));
  $a = (1,true,$x);
  @b = (1,true,$y);
  if ($a != @b) {
    return 1;
  }
  if ($a == (1,true,("ello",(-6)))) {
    return 1;
  }
}

test:tuple_binop_not_equals
{
  $x = ("hello",(-6));
  $y = ("hello",(-6));
  $a = (1,true,$x);
  @b = (1,true,$y);
  if ($a != @b) {
    return 1;
  }
  if (!($a != (1,true,("ello",(-6))))) {
    return 1;
  }
}
